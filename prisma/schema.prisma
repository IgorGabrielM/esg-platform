generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id         Int      @id @default(autoincrement())
  name       String
  email      String   @unique
  password   String
  imageBytes Bytes? // campo para armazenar a imagem em dados bin√°rios (opcional)
  createdAt  DateTime @default(now())
}
